class Student {
    String name;   // Instance variable
    int marks;     // Instance variable

    void setDetails(String n, int m) {  // Instance method
        name = n;  // Accessing instance variables
        marks = m;
    }

    void displayDetails() {  // Instance method
        System.out.println("Name: " + name + ", Marks: " + marks);
    }
}

public class Main {
    public static void main(String[] args) {
        Student s1 = new Student(); // Create object
        s1.setDetails("Rahul", 95);
        s1.displayDetails();

        Student s2 = new Student();
        s2.setDetails("Priya", 88);
        s2.displayDetails();
    }
}
